class Breakfast {
  init(who) {
    this.who = who;
    return;
    print "This is " + this.who + "'s breakfast";
  }

  cook() {
    print "Eggs a-fryin'!";
    return;
  }

  serve() {
    print "Enjoy your breakfast, " + this.who + ".";
  }
}

var brek = Breakfast("me");
print brek;

fun hello() {
  print "hello";
  return 1;
}

brek.hello = hello;
print brek.hello();

var serve = brek.serve;

brek.cook();
